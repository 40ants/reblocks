#!/bin/sh
#|-*- mode:lisp -*-|#
#| <Put a one-line description here>
exec ros -Q -- $0 "$@"
|#
(progn ;;init forms
  (ros:ensure-asdf)
  #+quicklisp
  (handler-case (ql:quickload :weblocks-test)
    (t ()
      (princ "Unable to load :WEBLOCKS-TEST")
      (terpri)
      (uiop:quit 3))))


(defpackage :ros.script.run-tests.3700466499
  (:use :cl))
(in-package :ros.script.run-tests.3700466499)


(defun main (&rest argv)
  (declare (ignorable argv))
  
  (ignore-errors
   (let ((result (weblocks-test:test-weblocks)))

     (if (lift:errors result)
         (uiop:quit 1)
         (uiop:quit 0))))
  
  ;; some error was raised by test-framework
  (uiop:quit 2))

